    "#예산\n",
    "def solution(d, budget):\n",
    "    answer = 0\n",
    "    \n",
    "    d.sort()\n",
    "    \n",
    "    for i in d:\n",
    "        if budget - i < 0:\n",
    "            break\n",
    "        budget = budget - i\n",
    "        answer+=1\n",
    "        \n",
    "    return answer"
   ]
