   "source": [
    "n = int(input())\n",
    "\n",
    "while True:\n",
    "    x = int(input())\n",
    "    if x == 0:\n",
    "        break\n",
    "    if x % n == 0:\n",
    "        print(\"{} is a multiple of {}.\".format(x, n))\n",
    "    else:\n",
    "        print(\"{} is NOT a multiple of {}.\".format(x, n))"
   ]
