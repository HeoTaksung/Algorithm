    "#문자열 내림차순으로 배치하기\n",
    "def solution(s):\n",
    "    answer = ''\n",
    "    result = list()\n",
    "    for i in s:\n",
    "        result.append(i)\n",
    "    result.sort()\n",
    "    result.reverse()\n",
    "    answer = ''.join(result)\n",
    "    return answer"
   ]
