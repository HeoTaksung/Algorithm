    "#x만큼 간격이 있는 n개의 숫자\n",
    "def solution(x, n):\n",
    "    answer = []\n",
    "    c = x\n",
    "    for i in range(n):\n",
    "        answer.append(c)\n",
    "        c += x\n",
    "    return answer"
   ]
  },
