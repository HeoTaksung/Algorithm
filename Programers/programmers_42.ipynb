   "source": [
    "#최댓값과 최솟값\n",
    "def solution(s):\n",
    "    li = s.split()\n",
    "    for i in range(len(li)):\n",
    "        li[i] = int(li[i])\n",
    "    answer = str(min(li))+ ' ' + str(max(li))\n",
    "    return answer"
   ]
  },
